{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/users/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/users/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/users",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "appointments": {
      "type": "application",
      "root": "apps/appointments",
      "entryFile": "main",
      "sourceRoot": "apps/appointments/src",
      "compilerOptions": {
        "tsConfigPath": "apps/appointments/tsconfig.app.json"
      }
    },
    "contracts": {
      "type": "library",
      "root": "libs/contracts",
      "entryFile": "index",
      "sourceRoot": "libs/contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/contracts/tsconfig.lib.json"
      }
    },
    "medical-records": {
      "type": "application",
      "root": "apps/medical-records",
      "entryFile": "main",
      "sourceRoot": "apps/medical-records/src",
      "compilerOptions": {
        "tsConfigPath": "apps/medical-records/tsconfig.app.json"
      }
    },
    "shared-auth": {
      "type": "library",
      "root": "libs/shared-auth",
      "entryFile": "index",
      "sourceRoot": "libs/shared-auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared-auth/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    }
  }
}